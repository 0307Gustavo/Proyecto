								<input type="radio"
								name="mujer" id="mujer"
								value="mujer"></input>
								<label>Mujer</label>
							</th>
						</tr>
						<tr>
							<th>
								<label>Correo electronico:</label>
								<input type="text" size="40"
								name="correo" id="correo"/>
							</th>
						</tr>
						<tr>
							<th>
								<label>Telefono:</label>
								<input type="text"
								name="Telefono" id="Telefono"/>
							</th>
						</tr>
						<tr >
							<th 
								<label>Mensaje:</label>
								<textarea rows="10" cols="30"
								name="mensaje" id="mensaje"/></textarea>
							</th>
						</tr>
						<tr>
							<th>
								<input type="submit"
								value="Enviar">
								<input type ="reset">
							</th>
						</tr>
					</table>
				</form>
